{
  "name": "typescript-babel-jest",
  "version": "0.1.5",
  "description": "TS to ES6 to ES5",
  "main": "index.js",
  "author": "Pavel Lozinsky <jingle@live.com> (http://github.com/lozinsky)",
  "license": "MIT",
  "keywords": [
    "typescript",
    "babel",
    "jest",
    "preprocesor",
    "typescript-babel-jest",
    "tests"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lozinsky/typescript-babel-jest"
  },
  "homepage": "https://github.com/lozinsky/typescript-babel-jest",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "start": "npm run build-clean && npm run build-start",
    "test": "npm run build-start && npm run tests-mock && npm run tests-jest && npm run tests-clean && npm run build-clean",
    "build-start": "babel source --presets babel-preset-es2015 --out-file index.js",
    "build-clean": "rm -rf ./index.js",
    "tests-mock": "sh ./misc/test-init.sh",
    "tests-jest": "jest --no-cache",
    "tests-clean": "sh ./misc/test-rm.sh",
    "travis": "npm run lint && npm test",
    "lint": "eslint source/* examples/* --ext .js"
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(js)$",
    "moduleFileExtensions": [
      "js"
    ]
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "babel-jest": "^15.0.0",
    "typescript": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^0.9.31",
    "@types/react": "^0.14.35",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.5.0",
    "eslint-plugin-node": "^2.0.0",
    "react": "^15.3.1",
    "react-test-renderer": "^15.3.2"
  }
}
